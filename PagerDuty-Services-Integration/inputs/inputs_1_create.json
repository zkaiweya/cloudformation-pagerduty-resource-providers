{
    "Type": "event_transformer_api_inbound_integration",
    "Name":"Email",
    "ServiceId": "P55XL09",
    "VendorId": "PZQ6AUS",
    "IntegrationEmail": "test@dev-aws-cloudformation-cloudsoft.pagerduty.com",
    "EmailIncidentCreation": "use_rules",
    "EmailFilterMode": "or-rules-email",
    "EmailParsingFallback": "discard",
    "EmailFilters": [
        {
            "SubjectMode": "match",
            "SubjectRegex": "alert",
            "BodyMode": "match",
            "BodyRegex": "alert",
            "FromEmailMode": "match",
            "FromEmailRegex": "alert"
        }
    ],
    "EmailParsers": [
        {
            "Action": "trigger",
            "MatchPredicate": {
                "Type": "any",
                "Children": [{
                    "Type": "exactly",
                    "Matcher": "Thistest",
                    "Part": "body"
                }]
            },
            "ValueExtractors": [
                {
                    "Type": "regex",
                    "Part": "subject",
                    "ValueName": "incident_key",
                    "Regex": "This (\\d+)"
                },
                {
                    "Type": "between",
                    "Part": "body",
                    "ValueName": "incident_body",
                    "StartsAfter": "start",
                    "EndsBefore": "end"
                }
            ]
        }
    ]
}
